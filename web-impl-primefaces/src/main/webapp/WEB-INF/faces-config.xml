<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>

		<navigation-case>
			<from-outcome>crudperson</from-outcome>
			<to-view-id>/crud/person.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>personsearch</from-outcome>
			<to-view-id>/search/person.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>editmanyperson</from-outcome>
			<to-view-id>/form/editmanyperson.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>test</from-outcome>
			<to-view-id>/test.xhtml</to-view-id>
			<redirect />
		</navigation-case>


	</navigation-rule>

</faces-config>
