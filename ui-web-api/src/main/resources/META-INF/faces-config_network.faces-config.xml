<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		
		<navigation-case>
			<from-outcome>uniformResourceLocatorListView</from-outcome>
			<to-view-id>/private/security/uniformresourcelocator/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>uniformResourceLocatorEditView</from-outcome>
			<to-view-id>/private/security/uniformresourcelocator/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>uniformResourceLocatorConsultView</from-outcome>
			<to-view-id>/private/security/uniformresourcelocator/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>computerListView</from-outcome>
			<to-view-id>/private/network/computer/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>computerEditView</from-outcome>
			<to-view-id>/private/network/computer/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>computerConsultView</from-outcome>
			<to-view-id>/private/network/computer/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>serviceListView</from-outcome>
			<to-view-id>/private/network/service/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>serviceEditView</from-outcome>
			<to-view-id>/private/network/service/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>serviceConsultView</from-outcome>
			<to-view-id>/private/network/service/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
