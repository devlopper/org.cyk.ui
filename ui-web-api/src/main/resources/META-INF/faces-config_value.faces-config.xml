<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		
		<navigation-case>
			<from-outcome>valueListView</from-outcome>
			<to-view-id>/private/value/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>valueEditView</from-outcome>
			<to-view-id>/private/value/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>valueConsultView</from-outcome>
			<to-view-id>/private/value/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>valuePropertiesListView</from-outcome>
			<to-view-id>/private/value/properties/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>valuePropertiesEditView</from-outcome>
			<to-view-id>/private/value/properties/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>valuePropertiesConsultView</from-outcome>
			<to-view-id>/private/value/properties/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>valueCollectionListView</from-outcome>
			<to-view-id>/private/value/collection/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>valueCollectionEditView</from-outcome>
			<to-view-id>/private/value/collection/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>valueCollectionConsultView</from-outcome>
			<to-view-id>/private/value/collection/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>measureListView</from-outcome>
			<to-view-id>/private/value/measure/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>measureEditView</from-outcome>
			<to-view-id>/private/value/measure/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>measureConsultView</from-outcome>
			<to-view-id>/private/value/measure/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
