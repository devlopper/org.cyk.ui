<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>eventListView</from-outcome>
			<to-view-id>/private/event/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventEditView</from-outcome>
			<to-view-id>/private/event/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventConsultView</from-outcome>
			<to-view-id>/private/event/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>eventPartyListView</from-outcome>
			<to-view-id>/private/event/eventparty/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventPartyEditView</from-outcome>
			<to-view-id>/private/event/eventparty/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventPartyConsultView</from-outcome>
			<to-view-id>/private/event/eventparty/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>eventMissedListView</from-outcome>
			<to-view-id>/private/event/eventmissed/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventMissedEditView</from-outcome>
			<to-view-id>/private/event/eventmissed/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventMissedConsultView</from-outcome>
			<to-view-id>/private/event/eventmissed/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>eventlist</from-outcome>
			<to-view-id>/private/event/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventCrudOne</from-outcome>
			<to-view-id>/private/event/crudOne.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>toolscalendar</from-outcome>
			<to-view-id>/private/event/agenda.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>notifications</from-outcome>
			<to-view-id>/private/event/notifications.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
