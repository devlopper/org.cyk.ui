<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Public Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>applicationSetup</from-outcome>
			<to-view-id>/public/__application__/install.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- ########################################### Private Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>applicationEditView</from-outcome>
			<to-view-id>/private/application/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>applicationListView</from-outcome>
			<to-view-id>/private/application/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>applicationConsultView</from-outcome>
			<to-view-id>/private/application/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personRelationshipEditView</from-outcome>
			<to-view-id>/private/party/person/relationship/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>personRelationshipListView</from-outcome>
			<to-view-id>/private/party/person/relationship/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>personRelationshipConsultView</from-outcome>
			<to-view-id>/private/party/person/relationship/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personRelationshipTypeRoleEditView</from-outcome>
			<to-view-id>/private/party/person/relationshiptyperole/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>personRelationshipTypeRoleListView</from-outcome>
			<to-view-id>/private/party/person/relationshiptyperole/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>personRelationshipTypeRoleConsultView</from-outcome>
			<to-view-id>/private/party/person/relationshiptyperole/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>medicalInformationsAllergyEditView</from-outcome>
			<to-view-id>/private/party/person/medical/informationsallergy/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>medicalInformationsAllergyListView</from-outcome>
			<to-view-id>/private/party/person/medical/informationsallergy/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>medicalInformationsAllergyConsultView</from-outcome>
			<to-view-id>/private/party/person/medical/informationsallergy/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>medicalInformationsMedicationEditView</from-outcome>
			<to-view-id>/private/party/person/medical/informationsmedication/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>medicalInformationsMedicationListView</from-outcome>
			<to-view-id>/private/party/person/medical/informationsmedication/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>medicalInformationsMedicationConsultView</from-outcome>
			<to-view-id>/private/party/person/medical/informationsmedication/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
