<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>dynamicCrudOne</from-outcome>
			<to-view-id>/__dynamic__/crudone.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>dynamicCrudMany</from-outcome>
			<to-view-id>/__dynamic__/crudmany.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>dynamichierarchy</from-outcome>
			<to-view-id>/__dynamic__/hierarchy.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>deploymentmanagement</from-outcome>
			<to-view-id>/__management__/deployment.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>toolscalendar</from-outcome>
			<to-view-id>/__tools__/calendar.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<application>
		<action-listener>org.primefaces.application.DialogActionListener</action-listener>
		<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
		<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
		<!--
		<message-bundle>ci.gouv.budget.solde.sigdcp.controller.resources.primefaces.i18n</message-bundle>
		<message-bundle>ci.gouv.budget.solde.sigdcp.controller.resources.i18n</message-bundle>
		-->
		<locale-config>
			<supported-locale>fr</supported-locale>
		</locale-config>
	</application>

</faces-config>
