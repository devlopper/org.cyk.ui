<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		
		<navigation-case>
			<from-outcome>security</from-outcome>
			<to-view-id>/private/security/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>licenseListView</from-outcome>
			<to-view-id>/private/security/license/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>licenseEditView</from-outcome>
			<to-view-id>/private/security/license/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>licenseConsultView</from-outcome>
			<to-view-id>/private/security/license/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>roleListView</from-outcome>
			<to-view-id>/private/security/role/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>roleEditView</from-outcome>
			<to-view-id>/private/security/role/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>roleConsultView</from-outcome>
			<to-view-id>/private/security/role/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountListView</from-outcome>
			<to-view-id>/private/security/useraccount/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>userAccountEditView</from-outcome>
			<to-view-id>/private/security/useraccount/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>userAccountConsultView</from-outcome>
			<to-view-id>/private/security/useraccount/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>useraccountchangepassword</from-outcome>
			<to-view-id>/private/security/useraccount/changepassword.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>useraccountlogout</from-outcome>
			<to-view-id>/public/security/logout.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
