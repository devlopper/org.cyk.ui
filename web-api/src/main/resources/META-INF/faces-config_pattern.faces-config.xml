<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>nestedSetNodeEditView</from-outcome>
			<to-view-id>/private/nestedset/node/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>nestedSetNodeListView</from-outcome>
			<to-view-id>/private/nestedset/node/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>nestedSetNodeConsultView</from-outcome>
			<to-view-id>/private/nestedset/node/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		
		
	</navigation-rule>

</faces-config>
