<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>actorConsultView</from-outcome>
			<to-view-id>/private/__role__/__manager__/actor/consult.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>editMedicalInformations</from-outcome>
			<to-view-id>/private/__tools__/crud/editmedicalinformations.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>editActorRelationship</from-outcome>
			<to-view-id>/private/__tools__/crud/editactorrelationship.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		
		
	</navigation-rule>

</faces-config>
