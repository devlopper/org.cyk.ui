<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>applicationEditView</from-outcome>
			<to-view-id>/private/application/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>applicationListView</from-outcome>
			<to-view-id>/private/application/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>applicationConsultView</from-outcome>
			<to-view-id>/private/application/consult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>applicationSetup</from-outcome>
			<to-view-id>/public/__application__/install.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
