<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	
	<factory>
	    <exception-handler-factory>
	        org.omnifaces.exceptionhandler.FullAjaxExceptionHandlerFactory
	    </exception-handler-factory>
	</factory>

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- ########################################### Private Navigations Cases ############################################### -->
		<navigation-case>
			<from-outcome>privateindex</from-outcome>
			<to-view-id>/private/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Member -->
		<navigation-case>
			<from-outcome>useraccountconsult</from-outcome>
			<to-view-id>/private/__role__/__member__/useraccountconsult.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>useraccountchangepassword</from-outcome>
			<to-view-id>/private/__role__/__member__/useraccountchangepassword.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Administrator -->
		<navigation-case>
			<from-outcome>administratorindex</from-outcome>
			<to-view-id>/private/__role__/__administrator__/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Manager -->
		<navigation-case>
			<from-outcome>referenceentity</from-outcome>
			<to-view-id>/private/__role__/__manager__/referenceentity.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Security Manager -->
		<navigation-case>
			<from-outcome>security</from-outcome>
			<to-view-id>/private/__role__/__securitymanager__/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>useraccounts</from-outcome>
			<to-view-id>/private/__role__/__securitymanager__/listuseraccount.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>useraccountcrudone</from-outcome>
			<to-view-id>/private/__role__/__securitymanager__/createuseraccount.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Setting Manager -->
		<navigation-case>
			<from-outcome>licenseRead</from-outcome>
			<to-view-id>/private/__role__/__settingmanager__/readlicense.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Tools -->
		<navigation-case>
			<from-outcome>dynamicCrudOne</from-outcome>
			<to-view-id>/private/__tools__/crud/crudone.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>dynamicCrudMany</from-outcome>
			<to-view-id>/private/__tools__/crud/crudmany.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>toolscalendar</from-outcome>
			<to-view-id>/private/__tools__/event/agenda.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventlist</from-outcome>
			<to-view-id>/private/__tools__/event/list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>eventCrudOne</from-outcome>
			<to-view-id>/private/__tools__/event/crudOne.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>personEditView</from-outcome>
			<to-view-id>/private/__tools__/crud/editperson.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<!--navigation-case>
			<from-outcome>personListView</from-outcome>
			<to-view-id>/private/__tools__/crud/listperson.jsf</to-view-id>
			<redirect />
		</navigation-case-->
		
		<navigation-case>
			<from-outcome>actorConsultView</from-outcome>
			<to-view-id>/private/__tools__/crud/consultactor.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>editMedicalInformations</from-outcome>
			<to-view-id>/private/__tools__/crud/editmedicalinformations.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>notifications</from-outcome>
			<to-view-id>/private/__tools__/event/notifications.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>toolsreport</from-outcome>
			<to-view-id>/private/__tools__/export/report.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>exportdatatableservlet</from-outcome>
			<to-view-id>/private/__tools__/export/_cyk_report_/_dynamicbuilder_/_jasper_/</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>reportservlet</from-outcome>
			<to-view-id>/private/__tools__/export/_cyk_report_/_business_/_jasper_/</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- ########################################### Public Navigation Cases ############################################### -->
		
		<!-- Application -->
		<navigation-case>
			<from-outcome>applicationSetup</from-outcome>
			<to-view-id>/public/__application__/install.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Security -->
		<navigation-case>
			<from-outcome>useraccountlogout</from-outcome>
			<to-view-id>/public/__security__/logout.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- errors -->
		<navigation-case>
			<from-outcome>outcomenotfound</from-outcome>
			<to-view-id>/public/__error__/unknowoutcome.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>outcomecannotrenderview</from-outcome>
			<to-view-id>/public/__error__/cannotrenderview.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>

	<application>
		<resource-handler>org.cyk.ui.web.api.handler.DynamicResourceHandler</resource-handler>
		<!-- Because of glassfish Null String Issue -->
		<el-resolver>org.cyk.ui.web.api.resolver.NullStringResolver</el-resolver>
	</application>

	<locale-config>
		<supported-locale>fr</supported-locale>
	</locale-config>

</faces-config>
